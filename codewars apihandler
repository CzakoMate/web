function Futtat(){
    let ki="";
let user=document.getElementById("beker").value;
const url = `https://www.codewars.com/api/v1/users/${user}`
  async function fetchData(url) {
    try {
        const response = await fetch(url);
        if (!response.ok) {
            throw new Error(`HTTP error! Status: ${response.status}`);
        }
        const data = await response.json();
        console.log(data);
        console.log(data.ranks.languages)
       ki+=JSON.stringify(data)
    } catch (error) {
        console.error('Error fetching data:', error);
    }
}


fetchData(`https://www.codewars.com/api/v1/users/${user}`);
document.getElementById("kimenet").innerHTML=ki;
}
