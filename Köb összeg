function cubeOdd(arr) {

// insert code here >.<
let valasz;
let szamok=0;

for(let i=0;i<arr.length;i++)
  {
    if(arr[i]%2!==0)
      {
        szamok+=arr[i]*arr[i]*arr[i];
        valasz=szamok;
      }
    if(isNaN(arr[i]))
    {
      valasz=undefined;
      break;
    }
  }
return valasz;
}
